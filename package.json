{
    "name": "mailchimp-helper",
    "productName": "MailChimp content generator",
    "version": "1.0.0",
    "description": "An app for easy content generation for MailChimp newsletters",
    "main": "src/index.js",
    "scripts": {
        "start": "concurrently \"npm:svelte-dev\" \"electron-forge start\"",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "echo \"No linting configured\"",
        "svelte-build": "rollup -c",
        "svelte-dev": "rollup -c -w",
        "svelte-start": "sirv public"
    },
    "keywords": [],
    "author": {
        "name": "Goran AlkoviÄ‡",
        "email": "goran.alkovic@hotmail.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [{
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "mailchimp_helper"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ]
        }
    },
    "dependencies": {
        "@mailchimp/mailchimp_marketing": "^3.0.15",
        "concurrently": "^5.3.0",
        "electron-reload": "^1.5.0",
        "electron-squirrel-startup": "^1.0.0",
        "mailchimp-api-v3": "^1.14.0",
        "querystring": "^0.2.0",
        "sirv-cli": "^1.0.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "6.0.0-beta.53",
        "@electron-forge/maker-deb": "6.0.0-beta.53",
        "@electron-forge/maker-rpm": "6.0.0-beta.53",
        "@electron-forge/maker-squirrel": "6.0.0-beta.53",
        "@electron-forge/maker-zip": "6.0.0-beta.53",
        "electron": "10.1.1",
        "@rollup/plugin-commonjs": "^14.0.0",
        "@rollup/plugin-node-resolve": "^8.0.0",
        "rollup": "^2.3.4",
        "rollup-plugin-livereload": "^1.0.0",
        "rollup-plugin-svelte": "^6.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.0.0"
    }
}